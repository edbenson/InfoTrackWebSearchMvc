@model IEnumerable<InfoTrackWebSearch.Models.WebSearch>

@{
    ViewData["Title"] = "Search history";
}

<h1>Search history</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Date/Time
            </th>
            <th>
                Search query
            </th>
            <th>
                Search limit
            </th>
            <th>
                Scan for Url
            </th>
            <th>
                Positions found
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SearchTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SearchQuery)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SearchCount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ScanForUrl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Positions)
            </td>
        </tr>
}
    </tbody>
</table>
